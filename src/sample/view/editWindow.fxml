<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="166.0" prefWidth="382.0" style="-fx-background-color: #000033; -fx-background-radius: 3;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.EditController">
   <children>
      <Label text="Описание:" textFill="WHITE">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="textDescription" />
      <Label text="Url адрес:" textFill="WHITE">
         <VBox.margin>
            <Insets bottom="5.0" top="10.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="textUrl" />
      <VBox prefHeight="200.0" prefWidth="100.0" />
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button maxWidth="65.0" minWidth="65.0" mnemonicParsing="false" onAction="#btnOk" text="Ok">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button cancelButton="true" defaultButton="true" maxWidth="65.0" minWidth="65.0" mnemonicParsing="false" onAction="#btnCancel" text="Cancel" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
